{
  "name": "lingui-react-router",
  "version": "0.1.8",
  "description": "Integration between Lingui and React Router app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdvorak/lingui-react-router"
  },
  "author": "Michal Dvořák",
  "license": "MIT",
  "keywords": [
    "lingui",
    "react-router",
    "i18n",
    "internationalization"
  ],
  "packageManager": "pnpm@10.17.1",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/lingui.client.d.ts",
      "import": "./dist/lingui.client.mjs",
      "require": "./dist/lingui.client.cjs"
    },
    "./server": {
      "types": "./dist/lingui.server.d.ts",
      "import": "./dist/lingui.server.mjs",
      "require": "./dist/lingui.server.cjs"
    },
    "./routes": {
      "types": "./dist/routes.d.ts",
      "import": "./dist/routes.mjs",
      "require": "./dist/routes.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "prepack": "pnpm run build",
    "pretest": "pnpm run build",
    "test": "pnpm -F ./test build"
  },
  "dependencies": {
    "@lingui/conf": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/react": "^5.5.0",
    "@react-router/dev": "^7.9.3",
    "negotiator": "^1.0.0",
    "react": "^19.1.1",
    "react-router": "^7.9.3"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.15",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1"
  }
}
