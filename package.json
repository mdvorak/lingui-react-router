{
  "name": "lingui-react-router",
  "version": "0.3.2",
  "description": "Integration between Lingui and React Router app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdvorak/lingui-react-router"
  },
  "author": "Michal Dvořák",
  "license": "MIT",
  "keywords": [
    "lingui",
    "react-router",
    "i18n",
    "internationalization"
  ],
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./negotiate": {
      "types": "./dist/server/negotiate.d.ts",
      "import": "./dist/server/negotiate.mjs",
      "require": "./dist/server/negotiate.cjs"
    },
    "./routes": {
      "types": "./dist/plugin/routes.d.ts",
      "import": "./dist/plugin/routes.mjs",
      "require": "./dist/plugin/routes.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.mjs",
      "require": "./dist/plugin/index.cjs"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit --strict",
    "build": "pnpm run typecheck && unbuild",
    "dev": "pnpm run typecheck && unbuild --watch",
    "prepack": "pnpm run build",
    "pretest": "pnpm run build",
    "coverage": "vitest run --coverage",
    "test": "vitest run && pnpm -F ./test build",
    "start": "pnpm run test && pnpm -F ./test start"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "negotiator": "^1.0.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.18.12",
    "@types/react": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.4",
    "cldr-core": "^47.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rollup": "^4.52.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.1.7",
    "vitest": "^4.0.4"
  },
  "peerDependencies": {
    "@lingui/conf": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/react": "^5.5.0",
    "@react-router/dev": "^7.9.3",
    "react": "^19.1.1",
    "react-router": "^7.9.3"
  }
}
