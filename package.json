{
  "name": "lingui-react-router",
  "version": "1.0.1",
  "description": "Integration between Lingui and React Router app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdvorak/lingui-react-router"
  },
  "author": "Michal Dvořák",
  "license": "MIT",
  "keywords": [
    "lingui",
    "react-router",
    "i18n",
    "internationalization"
  ],
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./negotiate": {
      "types": "./dist/server/negotiate.d.ts",
      "import": "./dist/server/negotiate.mjs",
      "require": "./dist/server/negotiate.cjs"
    },
    "./routes": {
      "types": "./dist/plugin/routes.d.ts",
      "import": "./dist/plugin/routes.mjs",
      "require": "./dist/plugin/routes.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.mjs",
      "require": "./dist/plugin/index.cjs"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.mjs",
      "require": "./dist/test/index.cjs"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "node -e 'require(\"fs\").rmSync(\"dist\", { recursive: true, force: true })'",
    "typecheck": "tsc --noEmit --strict",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "build": "pnpm run typecheck && pnpm run lint && unbuild",
    "dev": "pnpm run typecheck && unbuild --watch",
    "prepack": "pnpm run build",
    "coverage": "vitest run --coverage",
    "test": "pnpm run typecheck && pnpm run lint && vitest run",
    "test:all": "pnpm run test && unbuild && pnpm -F ./test build",
    "start": "pnpm run test && pnpm -F ./test start",
    "prepare": "husky"
  },
  "dependencies": {
    "cldr-core": "^48.0.0",
    "fast-glob": "^3.3.3",
    "negotiator": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@testing-library/react": "^16.3.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.19.0",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "rollup": "^4.52.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "@lingui/conf": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/react": "^5.5.0",
    "@react-router/dev": "^7.9.3",
    "react": "^19.1.1",
    "react-router": "^7.9.3"
  },
  "pnpm": {
    "overrides": {
      "source-map": "0.7.6"
    }
  }
}
