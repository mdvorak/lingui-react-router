{
  "name": "lingui-react-router",
  "version": "0.3.1",
  "description": "Integration between Lingui and React Router app",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdvorak/lingui-react-router"
  },
  "author": "Michal Dvořák",
  "license": "MIT",
  "keywords": [
    "lingui",
    "react-router",
    "i18n",
    "internationalization"
  ],
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/i18n.client.d.ts",
      "import": "./dist/i18n.client.mjs",
      "require": "./dist/i18n.client.cjs"
    },
    "./server": {
      "types": "./dist/i18n.server.d.ts",
      "import": "./dist/i18n.server.mjs",
      "require": "./dist/i18n.server.cjs"
    },
    "./routes": {
      "types": "./dist/routes.d.ts",
      "import": "./dist/routes.mjs",
      "require": "./dist/routes.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.mjs",
      "require": "./dist/plugin/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit --strict",
    "build": "pnpm run typecheck && unbuild",
    "dev": "pnpm run typecheck && unbuild --watch",
    "prepack": "pnpm run build",
    "pretest": "pnpm run build",
    "test": "pnpm -F ./test build",
    "start": "pnpm run test && pnpm -F ./test start"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "negotiator": "^1.0.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.18.12",
    "@types/react": "^19.2.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rollup": "^4.52.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "peerDependencies": {
    "@lingui/conf": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/react": "^5.5.0",
    "@react-router/dev": "^7.9.3",
    "react": "^19.1.1",
    "react-router": "^7.9.3",
    "vite": "^7.1.7"
  }
}
