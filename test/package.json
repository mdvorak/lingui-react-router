{
  "name": "lingui-react-router-test",
  "private": true,
  "type": "module",
  "scripts": {
    "clean:build": "node -e 'require(\"fs\").rmSync(\"build\", { recursive: true, force: true })'",
    "clean:types": "node -e 'require(\"fs\").rmSync(\".react-router\", { recursive: true, force: true })'",
    "clean": "pnpm run clean:build && pnpm run clean:types",
    "extract": "lingui extract",
    "prebuild": "react-router typegen && tsc --noEmit --strict && lingui extract --clean",
    "build": "react-router build",
    "postbuild": "vitest run",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@lingui/core": "^5.6.0",
    "@lingui/react": "^5.6.0",
    "@react-router/dev": "^7.9.6",
    "@react-router/node": "^7.9.6",
    "@react-router/serve": "^7.9.6",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6"
  },
  "devDependencies": {
    "@lingui/cli": "^5.6.0",
    "@lingui/conf": "^5.6.0",
    "@lingui/vite-plugin": "^5.6.0",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "happy-dom": "^20.0.10",
    "lingui-react-router": "workspace:*",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.10"
  },
  "devDependenciesMeta": {
    "lingui-react-router": {
      "inject": true
    }
  }
}
