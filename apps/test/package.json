{
  "name": "lingui-react-router-test",
  "private": true,
  "type": "module",
  "scripts": {
    "extract": "lingui extract --clean",
    "build": "react-router build",
    "pretest": "lingui extract && react-router typegen",
    "test": "tsc --noEmit --strict && vitest run --coverage",
    "predev": "lingui extract",
    "dev": "react-router dev",
    "clean": "node -e '[\"build\", \".react-router\", \"coverage\"].forEach(d => fs.rmSync(d, { recursive: true, force: true }))'",
    "start": "react-router-serve ./build/server/index.js"
  },
  "dependencies": {
    "@lingui/core": "^5.7.0",
    "@lingui/react": "^5.7.0",
    "@react-router/dev": "^7.11.0",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "isbot": "^5.1.32",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0"
  },
  "devDependencies": {
    "@lingui/cli": "^5.7.0",
    "@lingui/conf": "^5.7.0",
    "@lingui/vite-plugin": "^5.7.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "happy-dom": "^20.0.11",
    "lingui-react-router": "workspace:*",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  },
  "devDependenciesMeta": {
    "lingui-react-router": {
      "inject": true
    }
  }
}
